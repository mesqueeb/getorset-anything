{
  "name": "getorset-anything",
  "version": "1.1.0",
  "description": "Gets a value from a Map/Obj or sets an initial value when not found and returns that. TypeScript supported.",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.js"
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "lint": "tsc --noEmit && eslint ./src",
    "test": "vitest run",
    "build": "del-cli dist && tsc",
    "release": "npm run lint && npm run build && np"
  },
  "devDependencies": {
    "@cycraft/eslint": "^0.4.4",
    "@cycraft/tsconfig": "^0.1.2",
    "del-cli": "^6.0.0",
    "np": "^10.2.0",
    "vitest": "^3.2.4"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "javascript",
    "map",
    "mapgetset",
    "map-get-or-set",
    "object-get-or-set",
    "getorset-anything",
    "get-or-set",
    "js-map",
    "typescript",
    "map-setter",
    "object-setter",
    "obj-set-get",
    "map-util",
    "obj-util",
    "setget",
    "getset",
    "getter-setter",
    "setter-getter"
  ],
  "author": "Luca Ban - Mesqueeb (https://cycraft.co)",
  "funding": "https://github.com/sponsors/mesqueeb",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mesqueeb/getorset-anything.git"
  },
  "homepage": "https://github.com/mesqueeb/getorset-anything#readme",
  "bugs": "https://github.com/mesqueeb/getorset-anything/issues"
}
